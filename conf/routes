
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.DominionController.index
GET     /dominion                   controllers.DominionController.dominion
GET     /about                      controllers.DominionController.about
GET     /dominion/process/:input    controllers.DominionController.process(input)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

      