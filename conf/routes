
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.DominionController.index
GET     /dominion                   controllers.DominionController.dominion
GET     /dominion/about             controllers.DominionController.about
GET     /dominion/process           controllers.DominionController.process(input: String)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)